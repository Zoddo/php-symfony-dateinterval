{
    "name": "zoddo/dateinterval-bundle",
    "description": "A Symfony bundle for the DateInterval library for Doctrine.",
    "keywords": ["date", "interval"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io/"
        },
        {
            "name": "Zoddo",
            "email": "zoddo.ino@gmail.com"
        }

    ],
    "require": {
        "doctrine/orm": ">=2.2",
        "herrera-io/doctrine-dateinterval": ">=1.0,<2.0-dev",
        "symfony/symfony": ">=2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "replace": {
        "herrera-io/dateinterval-bundle":"*"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Symfony\\DateInterval": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "target-dir": "Herrera/Symfony/DateInterval"
}
